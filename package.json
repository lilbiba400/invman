{
  "name": "invman",
  "version": "1.0.0",
  "description": "Simple CLI-Based inventory manager for CS2 using [node-globaloffensive](https://github.com/DoctorMcKay/node-globaloffensive) and [node-steamuser](https://github.com/DoctorMcKay/node-steam-user)",
  "main": "main.js",
  "type": "module",
  "dependencies": {
    "axios": "1.13.2",
    "chartscii": "3.2.0",
    "cli-progress": "3.12.0",
    "globaloffensive": "3.2.0",
    "pkg": "5.8.1",
    "qrcode-terminal": "0.12.0",
    "steam-session": "1.9.4",
    "steam-user": "5.2.3"
  },
  "bin": "main.js",
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "./dist/"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
